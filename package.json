{
  "name": "crudify",
  "version": "1.0.0",
  "description": "all in one NPM module for CRUD operations",
  "main": "index.ts",
  "scripts": {
    "FirebaseListingJob": "node ../demo_data/Jobs/FirebaseListingJob.js",
    "MongoListingJob": "node ../demo_data/Jobs/MongoListingJob.js",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "convert": "node src/util/converter.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaeleeps/crudify.git"
  },
  "keywords": [
    "CRUD"
  ],
  "author": "Jaeyoung Lee",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/async": "^3.2.18",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.6",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.2"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@google-cloud/firestore": "^6.5.0",
    "async": "^3.2.4",
    "csv-parser": "^3.0.0",
    "csvtojson": "^2.0.10",
    "firebase": "^8.10.1",
    "firebase-admin": "^11.7.0",
    "fs": "^0.0.1-security",
    "mongodb": "^5.1.0"
  }
}
